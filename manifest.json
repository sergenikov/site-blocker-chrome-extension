{
  "name": "URL Grabber",
  "description": "Grabs urls and saves them to local chrome storage",
  "version": "2.0",
  "permissions": [
			"activeTab",
			"storage"
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_title": "URL grabber"
  },
  "manifest_version": 2
}
